{
  "name": "vscode-pike-lang",
  "displayName": "Pike Language",
  "description": "Pike Language syntax highlighting and snippets",
  "version": "0.2.0",
  "publisher": "undeadfish",
  "engines": {
    "vscode": "^1.16.0"
  },
  "categories": [
    "Programming Languages", 
    "Snippets"
  ],
  "keywords": [
    "pike",
    "pmod",
    "lpc",
    "programming language"
  ],
  "icon": "images/pike_logo.png",
  "author": {
    "name": "Gregg Reece"
  },
  "homepage": "https://github.com/undeadfish/vscode-pike-lang/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/undeadfish/vscode-pike-lang.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "pike",
        "aliases": [ "Pike", "pike" ],
        "extensions": [ ".pike", ".pmod" ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pike",
        "scopeName": "source.pike",
        "path": "./syntaxes/Pike.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "pike",
        "path": "./snippets/pike.json"
      }
    ]
  }
}
